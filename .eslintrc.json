{
    "root": true,
    "extends": [
        "plugin:import/warnings",
        "plugin:import/errors",
        "eslint:recommended"
    ],
    "env": {
        "commonjs": true,
        "browser": true,
        "mocha": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-this-before-super": "warn",
        "constructor-super": "warn",
        "no-const-assign": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "valid-typeof": "warn",
        "no-console": "warn",
        "no-undef": "warn"
    }
}
